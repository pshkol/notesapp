<%- include('./templates/header') %>

<% if (notes.length > 0) { %>

<% notes.forEach((e) => { %>

<div class="card" style="width: 18rem;">
  <div class="card-body">
    <h5 class="card-title"><%= e.title %></h5>
    <p class="card-text"><%= e.text %></p>
  </div>
</div>

<% }) %>

<% } %>

<%- include('./templates/footer') %>
